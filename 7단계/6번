#include <stdio.h>
#include<string.h>
#include<ctype.h>
#include<malloc.h>
#pragma warning(disable:4996)

int read_line(char str[], int limit)
{
	int ch, i = 0;
	while ((ch = getchar()) != '\n')
	{
		if (i < limit - 1)
			str[i++] = ch;
	}

	str[i] = '\0';
	return i;
}

int main()
{
	char s[1000000];

	read_line(s, 1000000);

	char delimit[] = " ";

	char* token;

	token = strtok(s, delimit);

	int count = 0;
	while (token != NULL)
	{
		token = strtok(NULL, delimit);
		count++;
	}
	printf("%d", count);

	return 0;
}
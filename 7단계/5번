#include <stdio.h>
#include<string.h>
#include<malloc.h>
#pragma warning(disable:4996)

char word[1000000];

int main()
{
	
	int alp[26] = { 0 };

	scanf("%s", word);

	int a = strlen(word);

	for (int i = 0; i < a; i++)
	{
		if (word[i] >= 97 && word[i] <= 122)
			word[i] -= 32;

		alp[word[i]-65]++;
	}

	int max = -1;
	int index = -1;
	int m = 0;
	for (int z = 0; z < 26; z++)
	{
		//printf("%c : %d\n",i+65, alp[i]);
		if (alp[z] > max)
		{
			max = alp[z];
			index = z;
			m = 0;
		}

		else if ((alp[z] == max)&&(alp[z]>0))//여기
		{
			m = 1;
		}
	}

	if (m == 0)printf("%c", index + 65);
	else
		printf("?");

	return 0;
}